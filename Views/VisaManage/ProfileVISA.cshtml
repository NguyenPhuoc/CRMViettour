@{
    ViewBag.Title = "Hồ sơ VISA";
}

<div class="page-content">
    <!-- BEGIN PAGE HEAD-->
    <div class="page-head">
        <!-- BEGIN PAGE TITLE -->
        <div class="page-title">
            <h1>HỒ SƠ VISA</h1>
        </div>
        <!-- END PAGE TITLE -->

    </div>
    <!-- END PAGE HEAD-->
    <!-- BEGIN PAGE BREADCRUMB -->
    <ul class="page-breadcrumb breadcrumb">
        <li>
            <a href="/">Trang chủ</a>
            <i class="fa fa-circle"></i>
        </li>
        <li>
            <span class="active">Page Layouts</span>
        </li>
    </ul>
    <!-- END PAGE BREADCRUMB -->
    <!-- BEGIN PAGE BASE CONTENT -->
    <div class="note note-info">
        <div class="row">
            <div class="col-md-7">
                <select class="form-control" id="customer-select2">
                    <option value="HN">Tour Hà Nội - Hạ Long 4N3Đ</option>
                    <option value="SG">Tour Đà Lạt - Nha Trang 6N5Đ</option>
                </select>
            </div>
            <div class="col-md-5">
                <div class="pull-right">
                    <button class="btn btn-default"><i class="fa fa-refresh"></i>&nbsp;Nạp</button>
                    <a class="btn btn-primary" data-toggle="modal" href="#modal-insert-profilevisa"><i class="fa fa-plus"></i>&nbsp;Thêm</a>
                    <a class="btn btn-warning" id="btnEdit" data-toggle="modal" href="#modal-edit-profilevisa"><i class="fa fa-pencil"></i>&nbsp;Sửa</a>
                    <button class="btn btn-danger" id="btnRemove" onclick="confirm('Bạn có chắc chắn xóa không?');"><i class="fa fa-remove"></i>&nbsp;Xóa</button>
                    <a class="btn green" data-toggle="modal" href="#modal-import-profilevisa"><i class="icon-action-redo"></i>&nbsp;Import</a>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="portlet light bordered">
                <div class="portlet-body">
                    <div class="table-responsive table-custom-format">
                        <table class="table table-striped table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th class="table-checkbox">
                                        <input type="checkbox" id="allcb" name="allcb" onclick="SelectAllCb()" />
                                    </th>
                                    <th id="CODE">Ký hiệu
                                        <a href="#" id="sortasc1" onclick="clickSortASC(1)" class="icon-sort-table"><i class="fa fa-sort-asc"></i></a>
                                        <a href="#" id="sortdesc1" onclick="clickSortDESC(1)" class="icon-sort-table"><i class="fa fa-sort-desc"></i></a>
                                    </th>
                                    <th id="TITLE">Tiêu đề
                                            <a href="#" id="sortasc2" onclick="clickSortASC(2)" class="icon-sort-table"><i class="fa fa-sort-asc"></i></a>
                                        <a href="#" id="sortdesc2" onclick="clickSortDESC(2)" class="icon-sort-table"><i class="fa fa-sort-desc"></i></a>
                                    </th>
                                    <th id="CUSTOMER">Khách hàng
                                            <a href="#" id="sortasc3" onclick="clickSortASC(3)" class="icon-sort-table"><i class="fa fa-sort-asc"></i></a>
                                        <a href="#" id="sortdesc3" onclick="clickSortDESC(3)" class="icon-sort-table"><i class="fa fa-sort-desc"></i></a>
                                    </th>
                                    <th id="COUNTRY">Quốc gia
                                            <a href="#" id="sortasc4" onclick="clickSortASC(4)" class="icon-sort-table"><i class="fa fa-sort-asc"></i></a>
                                        <a href="#" id="sortdesc4" onclick="clickSortDESC(4)" class="icon-sort-table"><i class="fa fa-sort-desc"></i></a>
                                    </th>
                                    <th id="NOTE">Diễn giải
                                            <a href="#" id="sortasc5" onclick="clickSortASC(5)" class="icon-sort-table"><i class="fa fa-sort-asc"></i></a>
                                        <a href="#" id="sortdesc5" onclick="clickSortDESC(5)" class="icon-sort-table"><i class="fa fa-sort-desc"></i></a>
                                    </th>
                                    <th id="STATUS">Trạng thái
                                            <a href="#" id="sortasc6" onclick="clickSortASC(6)" class="icon-sort-table"><i class="fa fa-sort-asc"></i></a>
                                        <a href="#" id="sortdesc6" onclick="clickSortDESC(6)" class="icon-sort-table"><i class="fa fa-sort-desc"></i></a>
                                    </th>
                                    <th id="STATUS1">Tình trạng
                                            <a href="#" id="sortasc7" onclick="clickSortASC(7)" class="icon-sort-table"><i class="fa fa-sort-asc"></i></a>
                                        <a href="#" id="sortdesc7" onclick="clickSortDESC(7)" class="icon-sort-table"><i class="fa fa-sort-desc"></i></a>
                                    </th>
                                    <th id="DEADLINE">Thời hạn nộp hồ sơ
                                            <a href="#" id="sortasc8" onclick="clickSortASC(8)" class="icon-sort-table"><i class="fa fa-sort-asc"></i></a>
                                        <a href="#" id="sortdesc8" onclick="clickSortDESC(8)" class="icon-sort-table"><i class="fa fa-sort-desc"></i></a>
                                    </th>
                                    <th id="EFFECTIVEDATE">Ngày hiệu lực
                                            <a href="#" id="sortasc9" onclick="clickSortASC(9)" class="icon-sort-table"><i class="fa fa-sort-asc"></i></a>
                                        <a href="#" id="sortdesc9" onclick="clickSortDESC(9)" class="icon-sort-table"><i class="fa fa-sort-desc"></i></a>
                                    </th>
                                    <th id="EXPIRATIONDATE">Ngày hết hạn
                                            <a href="#" id="sortasc10" onclick="clickSortASC(10)" class="icon-sort-table"><i class="fa fa-sort-asc"></i></a>
                                        <a href="#" id="sortdesc10" onclick="clickSortDESC(10)" class="icon-sort-table"><i class="fa fa-sort-desc"></i></a>
                                    </th>
                                    <th id="VISAGROUP">Thời gian dự kiến Visa đoàn
                                            <a href="#" id="sortasc11" onclick="clickSortASC(11)" class="icon-sort-table"><i class="fa fa-sort-asc"></i></a>
                                        <a href="#" id="sortdesc11" onclick="clickSortDESC(11)" class="icon-sort-table"><i class="fa fa-sort-desc"></i></a>
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td></td>
                                    <td>
                                        <input type="text" class="form-control" /></td>
                                    <td>
                                        <input type="text" class="form-control" /></td>
                                    <td>
                                        <input type="text" class="form-control" /></td>
                                    <td>
                                        <input type="text" class="form-control" /></td>
                                    <td>
                                        <input type="text" class="form-control" /></td>
                                    <td>
                                        <input type="text" class="form-control" /></td>
                                    <td>
                                        <input type="text" class="form-control" /></td>
                                    <td>
                                        <input type="text" class="form-control" /></td>
                                    <td>
                                        <input type="text" class="form-control" /></td>
                                    <td>
                                        <input type="text" class="form-control" /></td>
                                    <td>
                                        <input type="text" class="form-control" /></td>
                                </tr>
                                @for (int i = 0; i < 5; i++)
                                {
                                    <tr>
                                        <td>
                                            <input type="checkbox" id="cb@(i)" name="cb@(i)" onclick="CheckSelect()" />
                                        </td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                    <div class="row paddingtop10">
                        <div class="col-md-6 col-sm-6">
                            <div>
                                <label>
                                    Hiển thị
                                        <select class="form-control input-sm input-xsmall input-inline" id="sort-data">
                                            <option value="10">10</option>
                                            <option value="30">30</option>
                                            <option value="50">50</option>
                                        </select>
                                    trong 21230 kết quả
                                </label>
                            </div>
                        </div>
                        <div class="col-md-6 col-sm-6">
                            <div class="dataTables_paginate paging_bootstrap_full_number pull-right">
                                <ul class="pagination" style="visibility: visible;">
                                    <li class="prev" id="btnFirstPrev"><a title="First"><i class="fa fa-angle-double-left"></i></a></li>
                                    <li class="prev" id="btnPrev"><a title="Prev"><i class="fa fa-angle-left"></i></a></li>
                                    <li class="active" id="page1"><a onclick="clickpageData(1)">1</a></li>
                                    <li id="page2"><a onclick="clickpageData(2)">2</a></li>
                                    <li id="page3"><a onclick="clickpageData(3)">3</a></li>
                                    <li id="page4"><a onclick="clickpageData(4)">4</a></li>
                                    <li id="page5"><a onclick="clickpageData(5)">5</a></li>
                                    <li class="next" id="btnNext"><a title="Next"><i class="fa fa-angle-right"></i></a></li>
                                    <li class="next" id="btnLastNext"><a title="Last"><i class="fa fa-angle-double-right"></i></a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- END PAGE BASE CONTENT -->
</div>

@Html.Partial("_Partial_Insert_ProfileVisa")
@Html.Partial("_Partial_Edit_ProfileVisa")
@Html.Partial("_Partial_Import_ProfileVisa")