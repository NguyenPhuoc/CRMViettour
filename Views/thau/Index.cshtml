@{
    ViewBag.Title = "Quản lý thầu";
}

<div class="page-content">
    <!-- BEGIN PAGE HEAD-->
    <div class="page-head">
        <!-- BEGIN PAGE TITLE -->
        <div class="page-title">
            <h1>DANH SÁCH THẦU</h1>
        </div>
        <!-- END PAGE TITLE -->

    </div>
    <!-- END PAGE HEAD-->
    <!-- BEGIN PAGE BREADCRUMB -->
    <ul class="page-breadcrumb breadcrumb">
        <li>
            <a href="index.html">Home</a>
            <i class="fa fa-circle"></i>
        </li>
        <li>
            <span class="active">Page Layouts</span>
        </li>
    </ul>
    <!-- END PAGE BREADCRUMB -->
    <!-- BEGIN PAGE BASE CONTENT -->
    <div class="note note-info">
        <div class="row">
            <div class="col-md-3">
                <select class="form-control" id="program-period" @*multiple="multiple"*@>
                    <option value="0">-- Kỳ báo cáo --</option>
                </select>
            </div>
            <div class="col-md-3">
                <select class="form-control" id="program-month" @*multiple="multiple"*@>
                    <option value="0">-- Tháng --</option>
                </select>
            </div>
            <div class="col-md-12 margintop10">
                <button class="btn btn-default"><i class="fa fa-refresh"></i>&nbsp;Nạp</button>
                <button class="btn btn-primary" id="btnAdd" href="#modal-insert-program" data-toggle="modal"><i class="fa fa-plus"></i>&nbsp;Thêm</button>
                <button class="btn btn-warning" id="btnEdit" href="#modal-edit-program" data-toggle="modal"><i class="fa fa-pencil"></i>&nbsp;Sửa</button>
                <button class="btn btn-danger" id="btnRemove" onclick="confirm('Bạn có chắc chắn muốn xóa không?');"><i class="fa fa-remove"></i>&nbsp;Xóa</button>
                <button class="btn green" href="#modal-addcustomer-program" data-toggle="modal" id="btnAddCustomer"><i class="fa fa-user-plus"></i>&nbsp;Nhân viên</button>
                <button class="btn red" href="#modal-work-task" data-toggle="modal" id="btnWork"><i class="fa fa-cogs"></i>&nbsp;Xử lý</button>
                <button class="btn blue" id="btnAddTask" href="#modal-insert-tourtask" data-toggle="modal"><i class="fa fa-clipboard"></i>&nbsp;Phân công nhiệm vụ</button>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="portlet light bordered">
                <div class="portlet-body">
                    <div class="table-scrollable table-custom-format">
                        <table class="table table-striped table-bordered table-hover" id="">
                            <thead>
                                <tr>
                                    <th class="table-checkbox">
                                        <input type="checkbox" id="allcb" name="allcb" onclick="SelectAllCb()" />
                                    </th>
                                    <th>STT</th>
                                    <th>Tên thầu</th>
                                    <th>Mã KH</th>
                                    <th>Ngày nộp thầu</th>
                                    <th>Ngày mở</th>
                                    <th>Tiến độ</th>
                                    <th>Thời hạn</th>
                                    <th>Số lượng khách</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr class="odd gradeX">
                                    <td>
                                        <input type="checkbox" id="cb" name="cb" onclick="CheckSelect()" />
                                    </td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <div class="portlet light bordered">
                <div class="portlet-body">
                    <div class="tabbable-custom ">
                        <ul class="nav nav-tabs ">
                            <li class="active">
                                <a href="#tab1" data-toggle="tab">Diễn giải chi tiết</a>
                            </li>
                            <li>
                                <a href="#tab2" data-toggle="tab">Nhật ký xử lý</a>
                            </li>
                            <li>
                                <a href="#tab3" data-toggle="tab">Nhân viên</a>
                            </li>
                            <li>
                                <a href="#tab4" data-toggle="tab">Nhiệm vụ</a>
                            </li>
                            <li>
                                <a href="#tab5" data-toggle="tab">Lịch hẹn</a>
                            </li>
                            <li>
                                <a href="#tab6" data-toggle="tab">Dịch vụ</a>
                            </li>
                        </ul>
                        <div class="tab-content">
                            <div class="tab-pane active" id="tab1">
                                @*Diễn giải chi tiết*@
                                Yêu cầu cụ thể:...
                            </div>
                            <div class="tab-pane" id="tab2">
                                @*Nhật ký xử lý*@
                                <div class="table-scrollable table-custom-format">
                                    <table class="table table-striped table-bordered table-hover" id="">
                                        <thead>
                                            <tr>
                                                <th>STT</th>
                                                <th>Thời gian</th>
                                                <th>Diễn giải</th>
                                                <th>Người nhập</th>
                                                <th>Ngày nhập</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr class="odd gradeX">
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            <div class="tab-pane" id="tab3">
                                @*Nhân viên*@
                                <div class="table-scrollable table-custom-format">
                                    <table class="table table-striped table-bordered table-hover" id="">
                                        <thead>
                                            <tr>
                                                <th>STT</th>
                                                <th>Mã số</th>
                                                <th>Họ tên</th>
                                                <th>Vai trò</th>
                                                <th>Diễn giải</th>
                                                <th>#</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr class="odd gradeX">
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td>
                                                    <a href="#">Xóa</a>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            <div class="tab-pane" id="tab4">
                                @*Nhiệm vụ*@
                                <div class="table-scrollable table-custom-format">
                                    <table class="table table-striped table-bordered table-hover" id="table-task">
                                        <thead>
                                            <tr>
                                                <th>STT</th>
                                                <th>Mã</th>
                                                <th>Loại nhiệm vụ</th>
                                                <th>Tên</th>
                                                <th>Người thực hiện</th>
                                                <th>Phòng ban</th>
                                                <th>Ngày bắt đầu</th>
                                                <th>Ngày kết thúc</th>
                                                <th>Thời hạn</th>
                                                <th>Tiến độ</th>
                                                <th>Mức độ</th>
                                                <th>Ngày hoàn thành</th>
                                                <th>Nội dung</th>
                                                <th>Người giao</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr class="odd gradeX">
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            <div class="tab-pane" id="tab5">
                                @*Lịch hẹn*@
                                <button class="btn btn-primary" data-toggle="modal" href="#modal-insert-schedule"><i class="fa fa-calendar"></i>&nbsp;Thêm lịch hẹn</button>
                                <br /><br />
                                <div class="table-scrollable table-custom-format">
                                    <table class="table table-striped table-bordered table-hover" id="">
                                        <thead>
                                            <tr>
                                                <th>STT</th>
                                                <th>Tiêu đề</th>
                                                <th>Người thực hiện</th>
                                                <th>Địa điểm</th>
                                                <th>Diễn giải</th>
                                                <th>Ngày bắt đầu</th>
                                                <th>Ngày kết thúc</th>
                                                <th>Liên quan đến khách hàng</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr class="odd gradeX">
                                                <td>1</td>
                                                <td>Chốt HĐ CRM Viettours</td>
                                                <td>Võ Ái Duy</td>
                                                <td>Công ty DIP VIETNAM</td>
                                                <td>KH sang làm việc về quy trình xử lý và các chức năng. Chốt hợp đồng CRM.</td>
                                                <td>1/11/2015</td>
                                                <td>2/11/2015</td>
                                                <td></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            <div class="tab-pane" id="tab6">
                                <a href="#modal-insert-services" data-toggle="modal" class="btn btn-success"><i class="fa fa-plus"></i>&nbsp;Thêm dịch vụ</a>
                                <br />
                                <br />
                                @*Dịch vụ*@
                                <div class="table-scrollable table-custom-format">
                                    <table class="table table-striped table-bordered table-hover" id="table-service">
                                        <thead>
                                            <tr>
                                                <th>STT</th>
                                                <th>Mã</th>
                                                <th>Loại dịch vụ</th>
                                                <th>Tên</th>
                                                <th>Địa chỉ</th>
                                                <th>Người liên hệ</th>
                                                <th>Điện thoại</th>
                                                <th>Ghi chú</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr class="odd gradeX">
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- END PAGE BASE CONTENT -->
</div>

@Html.Partial("_Partial_InsertProgram")
@Html.Partial("_Partial_EditProgram")
@Html.Partial("_Partial_AddCustomerProgram")
@Html.Partial("_Partial_AddServices")
@Html.Partial("_Partial_AddSchedule")
@Html.Partial("_Partial_InsertTaskTour")
@Html.Partial("_Partial_WorkTask")

@section scripts{
    <script>
        CKEDITOR.replace("ScheduleNote");
        CKEDITOR.replace("ProgramNoteEdit");
        CKEDITOR.replace("ProgramNoteInsert");
        CKEDITOR.replace("WorkTaskNote");
        CKEDITOR.replace("TaskTourNote");
    </script>
}